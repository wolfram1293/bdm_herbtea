<Popup1@Popup>
    title: "ラベンダーの茶葉"
    id:pop
    size_hint: .8, .8

    BoxLayout:
        Label:
            font_size: 30
            text: "ラベンダーの茶葉 12"
        Button:
            font_size: 40
            size_hint: 0.5, 0.95
            text: "ブレンドを\n\n開始する"
            on_press: app.root.change_disp2(10); app.root.popup_close1(); app.root.move_dcmoter_gpio12();

